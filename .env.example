# База данных
DATABASE_URL=postgresql+asyncpg://bankruptcy_user:12345@localhost:5432/bankruptcy_scoring
DATABASE_URL_SYNC=postgresql+asyncpg://bankruptcy_user:12345@localhost:5432/bankruptcy_scoring

# Redis для кэширования и очередей
REDIS_URL=redis://localhost:6379/0

# Настройки API
API_V1_STR=/api/v1
PROJECT_NAME=Bankruptcy Scoring System
DEBUG=True

# Внешние API
FSSP_BASE_URL=https://fssp.gov.ru
FEDRESURS_API_URL=https://fedresurs.ru/backend/companies
ROSREESTR_BASE_URL=https://rosreestr.gov.ru
NALOG_BASE_URL=https://service.nalog.ru

# Настройки прокси
PROXY_LIST_FILE=data/proxies.txt
USE_PROXY=True
PROXY_TIMEOUT=30
MAX_RETRIES=3

# Настройки парсинга
REQUEST_DELAY=1.0
BATCH_SIZE=100
CONCURRENT_REQUESTS=10

# Пути к файлам
INPUT_DIR=data/input
OUTPUT_DIR=data/output
LOGS_DIR=data/logs

# Пороги скоринга
MIN_DEBT_AMOUNT=100000
HIGH_DEBT_THRESHOLD=250000
MIN_SCORE_THRESHOLD=50

# Доступные регионы (через запятую)
AVAILABLE_REGIONS="Москва", "Санкт-Петербург", "Татарстан", "Саратов", "Калуга", "Краснодар", "Екатеринбург", "Новосибирск"
